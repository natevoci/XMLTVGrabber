<XMLTVGrabber_Config>
  <BaseUrl>
    <URL>http://www.abc.net.au/tv/guide/abctvweekguide.htm</URL>
    <URL>http://www.abc.net.au/tv/guide/abc2weekguide.htm</URL>
    <!--<DateFormat>yyyy-MM-dd</DateFormat>-->
    <!--<Days>7</Days>-->
    <!--<Location>1</Location>-->
  </BaseUrl>

  <DownloadOptions>
    <Referer></Referer>
    <TimeOut>180</TimeOut>
    <RetryCount>3</RetryCount>
    <WorkingDir>pages</WorkingDir>
    <WorkingDirHoursToReuse>0</WorkingDirHoursToReuse>
  </DownloadOptions>

  <ParseBasePageInfo>
    <DateRegEx>&lt;h2 class=.*?&gt;.*?,(.*?)&lt;/H2&gt;</DateRegEx>
    <DateFormat>MMMM d</DateFormat>
    <ItemRegEx>&lt;td&gt;(\d+:\d+[ap]m)&lt;/td&gt;\s*&lt;td&gt;(.*?)(?:&lt;small&gt;\((g|pg|m|ma)\)&lt;/small&gt;)?(?:&lt;BR&gt;(.*?))?&lt;/td&gt;</ItemRegEx>
    <ItemMatchOrder>TNRD</ItemMatchOrder>
    <TimeFormat>h:mmtt</TimeFormat>
    <ChannelRegEx>&lt;h2&gt;(.*?) - 7 Day Guide&lt;/H2&gt;</ChannelRegEx>
  </ParseBasePageInfo>

<!--
ItemMatchOrder
I = prog ID
T = Time
C = Channel Name
N = Program Name
L = Duration
R = Rating
D = Description
G = Category / Genre
. = Ignore

Format of description ends with these optional parts
(end of desc) (category) (year) (Part x) (Rpt) G/PG/M/MA (L,A,S,V) CC WS
-->

<XMLCreation>
   <OutputFile>xmltv.xml</OutputFile>
   <TimeZone>AUTO</TimeZone>
</XMLCreation>

<WebScheudler>
   <URL>http://localhost:8429/servlet/EpgDataRes?action=14&amp;reload=1&amp;rescan=1&amp;email=1</URL>
   <DoReload>FALSE</DoReload>
   <OutputResponseData>TRUE</OutputResponseData>
</WebScheudler>

</XMLTVGrabber_Config>