<XMLTVGrabber_Config>
  <BaseUrl>
    <URL>http://www20.sbs.com.au/whatson/?date=(DATESTRING)&amp;channelID=(LOCATION)</URL>
    <DateFormat>yyyy-MM-dd</DateFormat>
	<Days>7</Days>
    <Location>1</Location>
  </BaseUrl>

  <DownloadOptions>
    <Referer></Referer>
    <TimeOut>180</TimeOut>
    <RetryCount>3</RetryCount>
    <WorkingDir>pages</WorkingDir>
	<WorkingDirHoursToReuse>0</WorkingDirHoursToReuse>
  </DownloadOptions>

  <ParseBasePageInfo>
    <ItemRegEx>&lt;TD class=programlist.*?&lt;strong&gt;([0-9]{1,2}:[0-9]{2}).*?&lt;strong&gt;(.*?)(?:(?&lt;!Movie)(?&lt;!Science): (.*?))?&lt;/strong&gt;.*?&lt;/strong&gt;--&gt;(.*?(?:(?&lt;=\([^\)]*\) )\(([^\)]*)\))?(?: \((\d\d\d\d)\))?( \(Part \d+\))?( \(Rpt\))?( (?:G|PG|M|MA))?( \(\w(?:,\w)*\))?( CC)?( WS)?\s*)&lt;!--</ItemRegEx>
    <ItemMatchOrder>TNSDG...R...</ItemMatchOrder>
    <TimeFormat>HH:mm</TimeFormat>
	<ChannelID>SBS</ChannelID>
  </ParseBasePageInfo>

<!--
ItemMatchOrder
I = prog ID
T = Time
C = Channel Name
N = Program Name
L = Duration
R = Rating
D = Description
G = Category / Genre
. = Ignore

Format of description ends with these optional parts
(end of desc) (category) (year) (Part x) (Rpt) G/PG/M/MA (L,A,S,V) CC WS
-->

<XMLCreation>
   <OutputFile>xmltv.xml</OutputFile>
   <TimeZone>AUTO</TimeZone>
</XMLCreation>

<WebScheudler>
   <URL>http://localhost:8429/servlet/EpgDataRes?action=14&amp;reload=1&amp;rescan=1&amp;email=1</URL>
   <DoReload>FALSE</DoReload>
   <OutputResponseData>TRUE</OutputResponseData>
</WebScheudler>

</XMLTVGrabber_Config>