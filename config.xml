<XMLTVGrabber_Config>

<BaseUrl>
   <Location>94</Location>
   <DateFormateRegEx>&lt;OPTION value=([A-Z0-9]{20,100})[ selected]*&gt;([^&lt;>]*)</DateFormateRegEx>
   <URL>http://tvguide.ninemsn.com.au/search/default.asp?region=(LOCATION)&amp;day=(DATESTRING)&amp;TimeZ=early&amp;type=fta&amp;search=true</URL>
   <URL>http://tvguide.ninemsn.com.au/search/default.asp?region=(LOCATION)&amp;day=(DATESTRING)&amp;TimeZ=morning&amp;type=fta&amp;search=true</URL>
   <URL>http://tvguide.ninemsn.com.au/search/default.asp?region=(LOCATION)&amp;day=(DATESTRING)&amp;TimeZ=afternoon&amp;type=fta&amp;search=true</URL>
   <URL>http://tvguide.ninemsn.com.au/search/default.asp?region=(LOCATION)&amp;day=(DATESTRING)&amp;TimeZ=night&amp;type=fta&amp;search=true</URL>
</BaseUrl>

<DownloadOptions>
   <Referer>http://tvguide.ninemsn.com.au</Referer>
   <TimeOut>180</TimeOut>
   <RetryCount>3</RetryCount>
   <WorkingDir>pages</WorkingDir>
</DownloadOptions>

<ParseBasePageInfo>
   <ItemRegEx>&lt;B&gt;([0-9]{1,2}:[0-9]{2} [a-z]{2})&lt;/B&gt;&lt;BR&gt;&lt;B&gt;([^&lt;&gt;]*)&lt;/B&gt;&lt;/TD&gt;&lt;TD vAlign=top align=left&gt;&lt;A onclick="pop\(([0-9]*)\);return false;" href="closeup/default.asp\?pid=[0-9]*" target=new&gt;&lt;B&gt;([^&lt;&gt;]*)&lt;/B&gt;&lt;/A&gt;&lt;FONT color=#666666&gt; \(([0-9]{1,3}) mins, Rated:([^\)]*)\) &lt;/FONT&gt;&lt;BR&gt;([^&lt;>]*)&lt;</ItemRegEx>
   <ItemMatchOrder>TCINLRD</ItemMatchOrder>
   <TimeFormat>dddd, d MMM h:mm tt</TimeFormat>
   <DetailsURL>http://tvguide.ninemsn.com.au/cu/default.asp?pid=(PID)</DetailsURL>
</ParseBasePageInfo>

<!--
ItemMatchOrder
I = prog ID
T = Time
C = Channel Name
N = Program Name
L = Duration
R = Rating
D = Desription
-->

<XMLCreation>
   <OutputDir>output</OutputDir>
   <TimeZone>AUTO</TimeZone>
</XMLCreation>

<WebScheudler>
   <URL>http://localhost:8080/servlet/EpgDataRes?action=14&amp;reload=1&amp;rescan=1&amp;email=1</URL>
   <DoReload>FALSE</DoReload>
</WebScheudler>

</XMLTVGrabber_Config>